#include <stdio.h>
 /*el programa al recibir datos de longitud y peso de un dinosaurio expresados en pies y libras, calcula los datos de este animal pero en metros (m) y kilogramos (kg), respectivamente */

int main ()
{
	float longitud, peso, pies, libras, kg, m;
	printf ("Indique el valor en pies: ");
	scanf ("%f",&pies);
	printf ("Indique valor en libras: ");
	scanf ("%f",&libras);

	peso = libras * 0,45 kg /1
	longitud = pies * 0,3048 m /1

	//printf ("resultado peso en kg: %f", peso);
	//printf ("resultado longitud en m: %f", longitud);

	return 0 ;
}
